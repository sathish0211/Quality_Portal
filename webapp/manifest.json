{
  "_version": "1.60.0",
  "sap.app": {
    "id": "qualityportal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "4855b6d2-6287-4b65-96e2-34df64c7b942"
    },
    "dataSources": {
      "ZSG_LOGIN_CDS_890_CDS": {
        "uri": "/sap/opu/odata/sap/ZSG_LOGIN_CDS_890_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "ZSG_INSPECTION_CDS_890_CDS": {
        "uri": "/sap/opu/odata/sap/ZSG_INSPECTION_CDS_890_CDS/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "qualityportal.i18n.i18n"
        }
      },
      "loginModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Request",
          "preload": true
        },
        "dataSource": "ZSG_LOGIN_CDS_890_CDS"
      },
      "inspectionModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Request",
          "preload": true
        },
        "dataSource": "ZSG_INSPECTION_CDS_890_CDS"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "qualityportal.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Login",
          "pattern": "",
          "target": [
            "Login"
          ]
        },
        {
          "name": "Dashboard",
          "pattern": "dashboard",
          "target": [
            "Dashboard"
          ]
        },
        {
          "name": "InspectionList",
          "pattern": "inspection-list",
          "target": [
            "InspectionList"
          ]
        },
        {
          "name": "ResultRecording",
          "pattern": "result-recording/{InspectionLot}",
          "target": [
            "ResultRecording"
          ]
        },
        {
          "name": "UsageDecision",
          "pattern": "usage-decision/{InspectionLot}",
          "target": [
            "UsageDecision"
          ]
        }
      ],
      "targets": {
        "Login": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Login",
          "viewName": "Login"
        },
        "Dashboard": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "InspectionList": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "InspectionList",
          "viewName": "InspectionList"
        },
        "ResultRecording": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ResultRecording",
          "viewName": "ResultRecording"
        },
        "UsageDecision": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UsageDecision",
          "viewName": "UsageDecision"
        }
      }
    },
    "rootView": {
      "viewName": "qualityportal.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}